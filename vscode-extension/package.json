{
  "name": "gram-vscode-extension",
  "displayName": "Gram Language Support",
  "description": "Syntax highlighting and language support for Gram recipe files",
  "version": "0.2.1",
  "publisher": "abiwab",
  "license": "GPL-3.0",
  "author": "abiwab",
  "repository": {
    "type": "git",
    "url": "https://github.com/abiwab/gram.git",
    "directory": "vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/abiwab/gram/issues"
  },
  "homepage": "https://github.com/abiwab/gram/tree/main/vscode-extension#readme",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gram",
        "aliases": [
          "Gram",
          "gram"
        ],
        "extensions": [
          ".gram"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gram",
        "scopeName": "source.gram",
        "path": "./syntaxes/gram.tmLanguage.json"
      }
    ]
  }
}
